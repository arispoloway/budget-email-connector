{
  "emailStorePath": "./emails.sqlite",
  "refresh_interval_seconds": 600,
  "email": {
    "type": "gmail",
    "credentialsJsonPath": "./credentials.json",
    "tokenJsonPath": "./token.json",
    "label": "budget",
    "headless": true
  },
  "parser": {
    "type": "email_router",
    "routes": [
      {
        "emails": ["paylah.alert@dbs.com", "ibanking.alert@dbs.com"],
        "parser": {
          "type": "dbs",
          "accountId": "<FILL ME IN>",
          "cardNumberMapping": {
            "1234": "<CARD_ACCOUNT_ID_1>",
            "5678": "<CARD_ACCOUNT_ID_2>"
          }
        }
      }
    ]
  },
  "notifier": {
    "type": "discord",
    "webhookUrl": "<FILL ME IN>"
  },
  "destination": {
    "type": "actual_budget",
    "url": "http://localhost:5006",
    "password": "<FILL ME IN>",
    "syncId": "<FILL ME IN>",
    "noteSuffix": " #email-connector"
  }
}
